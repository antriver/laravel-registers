{
    "name": "antriver/laravel-registers",
    "description": "A way to keep a simple list of models belonging to another model. e.g. Users that have liked a Post.",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Kuske",
            "homepage": "http://www.anthonykuske.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Antriver\\LaravelRegisters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Antriver\\LaravelRegisters\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
	"laravel/framework": "^8.0.0|^9.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.23",
        "mockery/mockery": "^1.4"
    }
}
